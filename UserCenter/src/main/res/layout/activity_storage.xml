<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.StorageActivity">
    <Button
        android:layout_centerInParent="true"
        android:id="@+id/btn_storage_test"
        android:text="测试存储框架"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </Button>
    <Button
        android:id="@+id/btn_storage_test2"
        android:text="测试存储框架隔离"
        android:layout_toLeftOf="@+id/btn_storage_test"
        android:layout_alignBottom="@+id/btn_storage_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </Button>
</RelativeLayout>